{
    "name": "inanepain/stdlib",
    "description": "Common classes, tools and utilities used throughout the inanepain libraries.",
    "type": "library",
    "license": "Unlicense",
    "homepage": "https://github.com/inanepain/stdlib",
    "readme": "README.adoc",
    "authors": [
        {
            "name": "Philip Michael Raab",
            "email": "philip@cathedral.co.za",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "philip@cathedral.co.za",
        "issues": "https://github.com/inanepain/stdlib/issues"
    },
    "keywords": [
        "inane",
        "stdlib",
        "json",
        "array",
        "exception",
        "notice",
        "config"
    ],
    "autoload": {
        "psr-4": {
            "Inane\\Stdlib\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "discard-changes": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "psr/container": "*"
    },
    "require-dev": {}
}
